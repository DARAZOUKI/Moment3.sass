{
  "name": "moment3.sass",
  "version": "1.0.0",
  "description": "Moment3",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel src/index.html --no-cache --no-source-maps",
    "build": "parcel build src/index.html --no-cache --no-source-maps",
    "transpile-js": "babel src -d dist --presets @babel/preset-env",
    "minify-js": "terser dist/java.js -o dist/java.min.js",
    "minify-css": "cssnano styles.css -o output.min.css",
    "optimize-images": "node optimize-images.js",
    "minify-html": "html-minifier --collapse-whitespace --remove-comments --remove-redundant-attributes --remove-script-type-attributes --remove-tag-whitespace --use-short-doctype --minify-css true --minify-js true --input-dir src --output-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "Moment3.sass"
  },
  "author": "Dania",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^10.4.17",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "cssnano": "^5.1.15",
    "events": "^3.3.0",
    "html-minifier": "^4.0.0",
    "parcel": "^2.11.0",
    "parcel-bundler": "^1.12.5"
  }
}
